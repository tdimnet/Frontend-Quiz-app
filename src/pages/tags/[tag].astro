---
import type { GetStaticPaths } from "astro";
import DefaultLayout from "../../layouts/DefaultLayout.astro"

export const getStaticPaths = (() => {
    return [
        { params: {tag: "astro"} },
        { params: {tag: "success"} },
        { params: {tag: "errors"} }
    ];
}) satisfies GetStaticPaths;

const { tag } = Astro.params
---

<DefaultLayout>
    hello from { tag }
</DefaultLayout>
